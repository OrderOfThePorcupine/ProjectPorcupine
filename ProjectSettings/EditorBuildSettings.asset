# Enhanced EditorBuildSettings YAML configuration
%YAML 1.1
%TAG !u! tag:unity3d.com,2011:
--- !u!1045 &1
EditorBuildSettings:
  # Internal Unity hide flags for the Build Settings asset
  m_ObjectHideFlags: 0

  # Indicates the internal version for serialized build settings
  serializedVersion: 2

  # Scenes list which will be included in the build
  m_Scenes:
    - enabled: 1
      path: "Assets/_Scenes_/MainMenu.unity"
    - enabled: 1
      path: "Assets/_Scenes_/_World.unity"
